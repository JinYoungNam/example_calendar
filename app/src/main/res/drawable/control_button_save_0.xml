<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" android:state_focused="false"  android:color="@color/color_Control_Button1_Up">
        <shape>
            <gradient
                android:startColor="@color/color_Control_Button1_Save_Up_Top_0"
                android:centerColor="@color/color_Control_Button1_Save_Up_Center_0"
                android:endColor="@color/color_Control_Button1_Save_Up_Bottom_0"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="@color/color_Control_Button1_Save_Line_0" />
            <corners
                android:radius="2dp" />
        </shape>
    </item>
</selector>